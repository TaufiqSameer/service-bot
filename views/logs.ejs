<!DOCTYPE html>
<html>
<head>
  <title>Robot Logs</title>
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet">
</head>
<body class="bg-light">
  <nav class="navbar navbar-expand-lg navbar-dark bg-dark">
    <div class="container-fluid">
      <a class="navbar-brand" href="#">Service Robot</a>
      <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav ms-auto">
          <li class="nav-item">
            <a class="nav-link" href="/">Home</a>
          </li>
          <li class="nav-item">
            <a class="nav-link active" href="/logs">Logs</a>
          </li>
          <li class="nav-item">
            <a class="nav-link" href="#">Control</a>
          </li>
        </ul>
      </div>
    </div>
  </nav>

  <div class="container mt-5">
    <h1 class="text-center mb-4">Robot Logs</h1>

    <% if (data && data.length > 0) { %>
      <% data.forEach(item => { %>
        <div class="card mb-3 shadow-sm">
          <div class="card-body">
            <p><strong>Battery:</strong> <%= item.battery %>%</p>
            <p><strong>Obstacle:</strong> <%= item.obstacle ? "Yes" : "No" %></p>
            <p><strong>Timestamp:</strong> <%= new Date(item.timestamp).toLocaleString() %></p>
          </div>
        </div>
      <% }) %>
    <% } else { %>
      <div class="alert alert-warning text-center">
        No logs available.
      </div>
    <% } %>
  </div>

  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>
